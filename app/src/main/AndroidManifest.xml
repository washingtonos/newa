<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.fiap.letsclean">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Apenas para testar aplicacao -->
        <activity android:name=".GettingstartedActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastroGrupoActivity"
            android:label="Cadastrar Grupo"
            android:theme="@style/AppTheme1" />
        <activity android:name=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GettingstartedActivity" />
        </activity>
        <activity android:name=".GrupoActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".SobreActivity" />
        <activity android:name=".RegistrarActivity" />
        <activity android:name=".Grupo_Detalhe" />
        <activity android:name=".ComodoActivity" />
        <activity android:name=".CadastrarComodoActivity" />
        <activity android:name=".ComodoDetalheActivity"></activity>
    </application>

</manifest>